    "ltgt": "2.2.1",
    "protocol-buffers": "4.1.0"
  },
  "devDependencies": {
    "remove-saucelabs-jobs-by-build": "1.0.20",
    "standard": "12.0.1",
    "tap-spec": "5.0.0",
    "tape": "4.9.1",
    "zuul": "3.12.0",
    "zuul-ngrok": "4.0.0"
  },
  "standard": {
    "ignore": [
      "test.js"
    ]
