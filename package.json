{
  "name": "range-emitter",
  "description": "range emitter",
  "keywords": [
    "leveldb",
    "levelup",
    "ltgt",
    "range",
    "protocol",
    "buffers",
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jameskyburz/range-emitter.git"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "main": "index.js",
  "version": "1.0.12",
  "scripts": {
    "prepublish": "standard",
    "preversion": "standard",
    "pretest": "standard",
    "test": "tape test.js | tap-spec",
    "test-browsers": "remove-saucelabs-jobs-by-build && zuul test.js"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "dependencies": {
    "brfs": "1.4.3",
    "duplexify": "3.5.0",
    "end-of-stream": "1.1.0",
    "length-prefixed-stream": "1.5.1",
    "ltgt": "2.1.3",
    "protocol-buffers": "3.2.1"
  },
  "devDependencies": {
    "remove-saucelabs-jobs-by-build": "1.0.18",
    "standard": "8.6.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "zuul": "3.11.1",
    "zuul-ngrok": "4.0.0"
  },
  "standard": {
    "ignore": [
      "test.js"
    ]
  },
  "license": "MIT"
