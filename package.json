{
  "name": "range-emitter",
  "description": "range emitter",
  "keywords": [
    "leveldb",
    "levelup",
    "ltgt",
    "range",
    "protocol",
    "buffers",
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jameskyburz/range-emitter.git"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "main": "index.js",
  "version": "1.0.20",
  "scripts": {
    "prepublish": "standard",
    "preversion": "standard",
    "pretest": "standard",
    "test": "tape test.js | tap-spec",
    "test-browsers": "remove-saucelabs-jobs-by-build && zuul test.js"
  },
  "browserify": {
